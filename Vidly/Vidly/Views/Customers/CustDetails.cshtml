@model Customer
@{
    ViewData["Title"] = "CustDetails";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<div class="container">
    <div class="row">
        <div class="col">
            <h2>@Model.Name</h2>
            <ul>
                <li>MemberShip Type: @Model.memberShipType.Name</li>
                @if (Model.Birthday != null)
                {
                    <li>BirthDay: @Model.Birthday</li>
                }
            </ul>
        </div>
    </div>
</div>

